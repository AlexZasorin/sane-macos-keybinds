{
    "title": "Swap Cmd + Tab with Ctrl + Tab",
    "rules": [
        {
            "description": "Swap Cmd + Tab with Ctrl + Tab",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "tab",
                        "modifiers": {
                            "mandatory": ["command"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "tab",
                            "modifiers": ["control"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "tab",
                        "modifiers": {
                            "mandatory": ["control"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "tab",
                            "modifiers": ["command"]
                        }
                    ]
                }
            ]
        }
    ]
}

